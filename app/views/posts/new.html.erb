<h1>New Post</h1>

<%= render "partials/error" %>

<%= form_with model: @post, url: user_posts_path(@user), local: true do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  <div>
    <%= form.label :text %>
    <%= form.text_area :text %>
  </div>
  <%= form.submit "Create Post", class: "btn btn-primary" %>
<% end %>